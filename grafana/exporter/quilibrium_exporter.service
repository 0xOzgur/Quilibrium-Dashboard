[Unit]
Description=Quilibrium Exporter Service
After=network.target

[Service]
User=<your_username>
Group=<your_username>
WorkingDirectory=/home/<your_username>/my_project
ExecStart=/home/<your_username>/quilibrium/exporter/venv/bin/python /home/<your_username>/quilibrium/exporter/quilibrium-node-exporter.py
Restart=always

[Install]
WantedBy=multi-user.target
